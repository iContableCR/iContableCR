<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_company_form_timbre_odontologico" model="ir.ui.view">
        <field name="name">res.company.view.form.timbre_odontologico</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="cr_electronic_invoice.view_company_form_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_qr_type']" position="after">
                <field name="url_timbre_odontologico" />
                <field name="usuario_timbre_odontologico" />
            </xpath>
        </field>
    </record>

</odoo>
