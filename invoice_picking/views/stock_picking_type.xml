<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_picking_type_form_invoice_picking" model="ir.ui.view">
        <field name="name">stock.picking.type.view.form.invoice_picking</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='warehouse_id']" position="after">
                <field name="is_return" attrs="{'invisible':[('code','!=','incoming')]}" />
            </xpath>
        </field>
    </record>

</odoo>
